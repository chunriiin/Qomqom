<h1>New Community</h1>

<%= form_for(@community) do |f| %>
    <div class="form-group"><%= f.text_field :name, class: "form-control form-control-white", placeholder: "name" %></div>
    <div class="form-group"><%= f.text_field :language, class: "form-control form-control-white", placeholder: "language" %></div>
    <div class="form-group"><%= f.submit class: "btn btn-primary",  title: ""%></div>
<% end %>


<%= link_to 'Back', communities_path %>
</div>